<?xml version="1.0" encoding="utf-8"?>
<venus-client>
	<!-- 服务接口列表 -->
	<services>
		<!-- 定义服务以及指定该服务提供方的远程相关配置 -->
		<!--
		属性说明:
		ipAddressList:地址列表，服务级属性，若不配则走注册中心，多个以;间隔，示例如ipAddressList="10.47.16.9:16800;10.47.16.10:16800"
		timeout:超时时间，服务级&方法级属性，单位ms,默认3000ms，示例如,timeout="5000"
		retries:重试次数，服务级&方法级属性，默认不重试，若配置则自动使用failover容错策略，示例如，retries="3"
		-->
		<!--<service type="com.chexiang.venus.demo.provider.HelloService" ipAddressList="10.47.17.238:16800"/>-->
		<!--
		SIT:10.32.172.25:16800;10.32.174.211:16800
		-->
		<service type="com.chexiang.venus.demo.provider.HelloService">
			<!--方法级配置，方法级若配置则会覆盖服务级配置-->
			<!--
			<method name="getHello" timeout="3000"/>
			-->
		</service>
		<service type="com.chexiang.venus.demo.provider.EchoService"/>
