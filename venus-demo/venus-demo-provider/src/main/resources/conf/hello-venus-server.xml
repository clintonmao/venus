<?xml version="1.0" encoding="utf-8"?>

<venus-server>
	<interceptors>
		<interceptor name="acl-interceptor" type="com.meidusa.venus.backend.interceptor.AccessControlInterceptor" />
		<interceptor name="validator-interceptor" type="com.meidusa.venus.backend.interceptor.ValidatorInterceptor" />
	</interceptors>

	<services>
		<!--
		ref:引用spring bean名称
		-->
		<!--interceptors="acl-interceptor,validator-interceptor"-->
		<service type="com.chexiang.venus.demo.provider.HelloService" ref="helloService" supportVersion="{0,1,2,3,4,5,6}" interceptors="acl-interceptor"/>
		<service type="com.chexiang.venus.demo.provider.EchoService" ref="echoService" supportVersion="{1}"/>
		<service type="com.saic.framework.message.UniMessageService" ref="uniMessageService"/>
	</services>
</venus-server>